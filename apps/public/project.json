{
  "name": "public",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/public/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro dev",
        "cwd": "apps/public"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro build",
        "cwd": "apps/public"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro preview",
        "cwd": "apps/public"
      }
    }
  },
  "tags": []
}

